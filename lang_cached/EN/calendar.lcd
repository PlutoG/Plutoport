a:158:{s:15:"calendar_minute";s:8:"%i:%M %p";s:26:"calendar_minute_no_seconds";s:3:":00";s:26:"calendar_minute_ampm_known";s:5:"%i:%M";s:13:"calendar_date";s:5:"Y-m-d";s:21:"calendar_date_verbose";s:8:"l jS F Y";s:26:"calendar_date_range_single";s:10:"g:i a (jS)";s:31:"calendar_date_range_single_long";s:12:"g:i a (jS M)";s:21:"calendar_day_of_month";s:2:"jS";s:29:"calendar_day_of_month_verbose";s:4:"l jS";s:22:"calendar_month_in_year";s:5:"%b %Y";s:30:"calendar_month_in_year_verbose";s:5:"%B %Y";s:15:"EVENT_CONTINUES";s:17:"(event continues)";s:31:"BLOCK_side_calendar_DESCRIPTION";s:42:"Show a calendar box for the current month.";s:23:"BLOCK_side_calendar_USE";s:100:"Provides a very brief overview on calendar entries, with links to the full calendar for more detail.";s:31:"BLOCK_side_calendar_PARAM_param";s:174:"The type of interface to show. Must be either 'month' or 'listing' ("month" shows a calendar grid for the month, "listing" shows upcoming events in a list). Default: 'month'.";s:30:"BLOCK_side_calendar_PARAM_days";s:99:"The number of days shown on the view (only relevant for the 'listing' display type). Default: '30'.";s:30:"BLOCK_side_calendar_PARAM_zone";s:61:"The zone to look for the calendar module in. Default: search.";s:32:"BLOCK_side_calendar_PARAM_filter";s:63:"A comma-separated list of event types to include. Default: all.";s:31:"BLOCK_side_calendar_PARAM_title";s:54:"Title for the box. Default: based on language strings.";s:16:"MANAGE_CALENDARS";s:16:"Manage calendars";s:12:"DOC_CALENDAR";s:363:"A calendar is provided in the software, which also features an advanced scheduling system. The calendar may be used as:\n - a normal calendar\n - a personal diary/schedule\n - a system command scheduling system (automatically executed background tasks)\n - [i]or a mixture of the above, depending on the properties chosen for the various events in the system.[/i]";s:16:"PT_view_calendar";s:34:"View calendar (view shared events)";s:20:"PT_add_public_events";s: